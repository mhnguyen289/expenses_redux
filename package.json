{
  "name": "expenses-redux",
  "version": "1.0.0",
  "description": "An app that splits and records expenses, inspired by SplitWise",
  "main": "webpack.config.babel.js",
  "scripts": {
    "start": "npm-run-all --parallel webpack:watch lint:watch ",
    "webpack": "./node_modules/.bin/webpack -d",
    "webpack:watch": "./node_modules/.bin/webpack -w -d",
    "lint": "node_modules/.bin/esw webpack.config.* reactjsapp",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "build": "babel-node reactjsapp_utility/build.js",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evefonwu/expenses-redux.git"
  },
  "author": "Eve Fon Wu <evefonwu@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "colors": "1.1.2",
    "jquery": "3.1.1",
    "moment": "2.17.1",
    "object-assign": "4.1.0",
    "react": "15.4.1",
    "react-bootstrap": "0.30.7",
    "react-bootstrap-date-picker": "4.0.0",
    "react-dom": "15.4.1",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-logger": "2.7.4",
    "redux-thunk": "2.1.0",
    "webpack": "1.14.0"
  },
  "devDependencies": {
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "expect": "1.19.0",
    "jest": "18.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5"
  },
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  }
